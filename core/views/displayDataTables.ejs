<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style.css">
    <title>Data Tables</title>
</head>

<body>
    <div class="table-block">
        <h1>
            Temperature Table
        </h1>
        <main>
            <table>
                <thead>
                    <tr>
                        <th>Sensor Id</th>
                        <th>Value</th>
                        <th>Unit</th>
                        <th>Measure Time</th>
                    </tr>
                </thead>
                <tbody>
                    <% tempData.forEach( function(tempData) { %>
                        <tr>
                            <td data-title='Sensor Id'>
                                <%= tempData.sensor_id %>
                            </td>
                            <td data-title='Value'>
                                <%= tempData.value %>
                            </td>
                            <td data-title='Unit'>
                                <%= tempData.unit %>
                            </td>
                            <td data-title='Measure Time'>
                                <%= tempData.measure_at %>
                            </td>
                        </tr>
                        <% }); %>
                </tbody>
            </table>
        </main>
    </div>
